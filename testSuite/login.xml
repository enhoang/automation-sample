    <parameter name="browser" value="chrome"/>       <!-- chrome / firefox / safari -->
    <parameter name="ui.enabled" value="true"/>      <!-- true = enable UI, false = headless -->
    <parameter name="wait.timeout" value="10"/>      <!-- default timeout -->
    <test name="Login">
        <!-- IMPORTANT: Before running this test flow, make sure your DATA/CONFIGURATION is correct. -->
        <classes>
            <class name="src.customer.a0_pages.LoginByPhone"/>
        </classes>
    </test>
</suite>